<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC
		"Devocative/Demeter Module"
		"http://www.devocative.org/dtd/demeter-module.dtd">

<module
		shortName="${ModuleShortName.toUpperCase()}"
		mainResource="${package}.web.${ModuleName}DModule"
		configKeyClass="${package}.${ModuleName}ConfigKey"
		privilegeKeyClass="${package}.${ModuleName}PrivilegeKey">

	<entities>
		<entity type="${package}.entity.Book"/>
	</entities>

	<tasks>
	</tasks>

	<dPages>
		<dPage type="${package}.web.dpage.BookListDPage"
			   title="Book List"
			   uri="books"
			   inMenu="true"
			   roles="User"/>

		<!--<dPage type=""
			   title="KEY:"
			   uri="/"
			   inMenu=""
			   roles="Root|Admin|User"
				/>-->
	</dPages>
